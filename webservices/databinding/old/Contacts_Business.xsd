<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" >
	<xsd:include schemaLocation="Contacts_Business_Type.xsd"/>
	<xsd:element name="business">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="profile" type="business_type" minOccurs="1" maxOccurs="1" nillable="true"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="business_list">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="profiles" type="business_type" minOccurs="0" maxOccurs="unbounded" nillable="true"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>