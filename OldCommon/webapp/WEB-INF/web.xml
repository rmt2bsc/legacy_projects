<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <display-name>Common</display-name>
  <env-entry>  	
     <env-entry-name>APP_NAME</env-entry-name>  	
     <env-entry-value>common</env-entry-value>  	
     <env-entry-type>java.lang.String</env-entry-type>  
  </env-entry>    
  <listener>
    <listener-class>
      com.util.SessionListener
    </listener-class>
  </listener>
  <servlet>
    <servlet-name>sysconfig</servlet-name>
    <servlet-class>com.servlet.SystemConfigurator</servlet-class>
    <load-on-startup>5</load-on-startup>
  </servlet>
  <servlet>
	    <servlet-name>reqeustProcessor</servlet-name>
	    <servlet-class>com.servlet.SecureController</servlet-class>
	    <init-param>
	      <param-name>ConfigFileKey</param-name>
	      <param-value>web_app_mapping</param-value>
	    </init-param>
  </servlet>
  <servlet>
	    <servlet-name>unsecureRequestProcessor</servlet-name>
	    <servlet-class>com.servlet.UnsecureController</servlet-class>
        <init-param>
	      <param-name>ConfigFileKey</param-name>
	      <param-value>web_app_mapping</param-value>
	    </init-param>
  </servlet>  
  <servlet>
	  <servlet-name>reportProcessor</servlet-name>
	  <servlet-class>com.servlet.ReportingServlet</servlet-class>
        <init-param>
	      <param-name>ConfigFileKey</param-name>
	      <param-value>web_app_mapping</param-value>
	    </init-param>	  
  </servlet>
  <servlet>
    <servlet-name>services</servlet-name>
    <servlet-class>com.servlet.HttpRemoteServicesController</servlet-class>
  </servlet>  
  <servlet>
    <servlet-name>debugservlet</servlet-name>
    <servlet-class>com.servlet.DebugServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>usermaintservlet</servlet-name>
    <servlet-class>com.servlet.UserMaintServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>contactservlet</servlet-name>
    <servlet-class>com.servlet.ContactsServlet</servlet-class>
  </servlet>  
  <servlet>
		<servlet-name>glacctcatgmaintservlet</servlet-name>
		<servlet-class>com.servlet.GlAcctCategoryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>glaccountmaintservlet</servlet-name>
    <servlet-class>com.servlet.GlAccountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>com.servlet.TestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sysconfig</servlet-name>
    <url-pattern>/sysconfig</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	    <servlet-name>reqeustProcessor</servlet-name>
	    <url-pattern>/reqeustProcessor/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	    <servlet-name>unsecureRequestProcessor</servlet-name>
	    <url-pattern>/unsecureRequestProcessor/*</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
    <servlet-name>reportProcessor</servlet-name>
    <url-pattern>/reportProcessor/*</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
    <servlet-name>services</servlet-name>
    <url-pattern>/services</url-pattern>
  </servlet-mapping>    
  <servlet-mapping>
    <servlet-name>usermaintservlet</servlet-name>
    <url-pattern>/usermaintservlet</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
    <servlet-name>contactservlet</servlet-name>
    <url-pattern>/contactservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>glacctcatgmaintservlet</servlet-name>
    <url-pattern>/glacctcatgmaintservlet</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
    <servlet-name>glaccountmaintservlet</servlet-name>
    <url-pattern>/glaccountmaintservlet</url-pattern>
  </servlet-mapping>   
  <servlet-mapping>
    <servlet-name>debugservlet</servlet-name>
    <url-pattern>/debugservlet</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>    
  <welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
  </welcome-file-list>
	<resource-ref>
      <description>Common Database Connection</description>
      <res-ref-name>jdbc/common</res-ref-name>
      <res-type>javax.sql.DataSource</res-type>
      <res-auth>Container</res-auth>
  </resource-ref>
</web-app>
