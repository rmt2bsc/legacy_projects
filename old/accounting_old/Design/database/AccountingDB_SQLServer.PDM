     posh.[{D7474E47-859C-4349-8413-3B5CF17A3615}=purchase_order_status_id] status_id,
     po.[{CCA38CC8-CD06-49B2-BC74-A0A312278EE6}=ref_no] ref_no,
     (select sum(qty * unit_cost) from purchase_order_items where purchase_order_id = po.id) total,
