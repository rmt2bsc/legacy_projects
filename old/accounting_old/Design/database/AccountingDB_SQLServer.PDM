     posh.[{4EF6786A-16ED-449F-864F-33A650E3C455}=effective_date] effective_date,
     posh.[{138A19F3-FF94-44B2-A936-346DAE9EA63D}=end_date] end_date,
     posh.[{3510AD0D-4FBF-4548-8363-BD0C9A8D4CF6}=user_id] user_id,
     c.[{1C3F7F95-D211-40E1-8740-32EA2FBD5970}=account_number] account_number,
     c.[{2EE05481-9F21-42BD-A78B-1FBB02E7879C}=credit_limit] credit_limit,
     c.[{A85ADE48-2AF9-4AEF-86DD-F97E33F2D8BF}=creditor_type_id] credit_type_id,
     ct.[{DE790F0E-19BF-4A34-8958-402FC34A4B08}=description] creditor_type_id,
     c.[{1B6D292D-810A-423D-83D4-FFA760AAB63B}=business_id]
from
     [{5BE6E1B8-80F9-48B1-8C16-D05E5D1FED42}=purchase_order] po,
     [{0B8F9678-F05F-4EBB-A473-7DDFB1E81D6C}=purchase_order_status] pos,
     [{3E735504-7024-424F-A178-ED5397DDD5ED}=purchase_order_status_hist] posh,
