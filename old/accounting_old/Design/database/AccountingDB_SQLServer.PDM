<a:ModificationDate>1215194063</a:ModificationDate>
<a:Modifier>appdev</a:Modifier>
<a:Comment>Queries the database for those transactions that exist with and without creditor/vendors</a:Comment>
<a:View.SQLQuery>select xact.id xact_id,
       xact.xact_amount,
       xact.posted_date,
       xact.confirm_no,
       xact.neg_instr_no,
       xact.tender_id,
       xact_codes.description tender_description,
       xact.xact_type_id,
       xact_type.description xact_type_name,
       xact.reason,
       xact.xact_date,
       xact.date_created create_date,
       xact.user_id,
       xact_type.xact_category_id,
       0 xact_type_item_xact_type_id,
       0 creditor_id,
       &#39;&#39; account_number,
       0 creditor_type_id,
       0 credit_limit
 from 
      xact left outer join xact_codes on xact.tender_id = xact_codes.id 
           inner join xact_type on xact.xact_type_id = xact_type.id
Where 
      not exists (select 1 from creditor_activity where xact_id = xact.id)

union

select xact.id xact_id,
       xact.xact_amount,
       xact.posted_date,
       xact.confirm_no,
       xact.neg_instr_no,
       xact.tender_id,
       xact_codes.description tender_description,
       xact.xact_type_id,
       xact_type.description xact_type_name,
       xact.reason,
       xact.xact_date,
       xact.date_created create_date,
       xact.user_id,
       xact_type.xact_category_id,
       0 xact_type_item_xact_type_id,
       creditor.id creditor_id,
       creditor.account_number,
       creditor.creditor_type_id,
       creditor.credit_limit
From xact left outer join xact_codes on xact.tender_id = xact_codes.id
          inner join xact_type on xact.xact_type_id = xact_type.id
          inner join creditor_activity on creditor_activity.xact_id = xact.id
          inner join creditor on creditor_activity.creditor_id = creditor.id
          inner join creditor_type on creditor_type.id = creditor.creditor_type_id
</a:View.SQLQuery>
