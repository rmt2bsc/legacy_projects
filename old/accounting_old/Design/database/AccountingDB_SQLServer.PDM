     [{8E88982B-88CF-46E5-95D6-E7C64B629125}=creditor_type] ct

where
      po.[{390F130B-327B-4693-8453-9BA397EF7F67}=id] = posh.[{1B7C4D12-A448-4710-B8D4-F078557F95C9}=purchase_order_id]
  and pos.[{1948C955-8D55-4939-8A5D-C56BF605AE0B}=id] = posh.[{D7474E47-859C-4349-8413-3B5CF17A3615}=purchase_order_status_id]
  and posh.[{138A19F3-FF94-44B2-A936-346DAE9EA63D}=end_date] is null
  and po.[{6A45E135-D22E-4EB4-863E-45710F9CD969}=vendor_id] = c.[{D266E92A-F3A2-4761-9F8B-E82BF3BFCF6C}=id]
